{
  "name": "otrm-starter",
  "version": "0.1.2",
  "homepage ": "/otrm-starter/",
  "private": true,
  "dependencies": {
    "cra-template": "1.1.2",
    "otrm": "^0.1.24",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "PUBLIC_URL=/otrm-starter react-scripts build && cp build/index.html build/404.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "PUBLIC_URL=/otrm-starter react-scripts build && cp build/index.html build/404.html",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.13.13",
    "gh-pages": "^3.1.0"
  }
}
